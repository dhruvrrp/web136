@{
    ViewBag.Title = "NewInstructor";
    var id = ViewBag.id;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>New Instructor</h3>

<div id="divInstructor">
    <a href="~/Admin/Index?id=@ViewBag.id">Back</a><br /><br />
    <table>
        <tr>
            <td>Id</td>
            <td><input data-bind="value: id" /> (at least 5 chars)</td>
        </tr>
        <tr>
            <td>First</td>
            <td><input data-bind="value: first" /></td>
        </tr>
        <tr>
            <td>Last</td>
            <td><input data-bind="value: last" /></td>
        </tr>
        <tr>
            <td>Title</td>
            <td><input data-bind="value: title" /></td>
        </tr>
        <tr>
            <td></td>
            <td><button data-bind="click: add">Add</button></td>
        </tr>
    </table>
</div>

<script type="text/javascript">
    console.log("!!! before load");
    $(document).ready(function () {
        console.log("load complete");
        require(['ViewModels/InstructorViewModel'], function (instructorViewModel) {
            console.log("Initializing");
            var InstructorViewModelObj = new instructorViewModel();
            InstructorViewModelObj.Initialize();
        });
    });
</script>