<div id="divEditStudent">
    <table>
        <tr>
            <td>Id</td>
            <td>@id</td>
        </tr>
        <tr>
            <td>SSN</td>
            <td><input data-bind="value: ssn" /></td>
        </tr>
        <tr>
            <td>First</td>
            <td><input data-bind="value: first" /></td>
        </tr>
        <tr>
            <td>Last</td>
            <td><input data-bind="value: last" /></td>
        </tr>
        <tr>
            <td>Email</td>
            <td><input data-bind="value: email" /></td>
        </tr>
        <tr>
            <td>Password</td>
            <td><input type="password" data-bind="value: password" /></td>
        </tr>
        <tr>
            <td>Shoe Size</td>
            <td><input data-bind="value: shoesize" /></td>
        </tr>
        <tr>
            <td>Weight</td>
            <td><input data-bind="value: weight" /></td>
        </tr>
        <tr>
            <td></td>
            <td><button data-bind="click: edit">Edit</button></td>
        </tr>
    </table>
</div>

<script type="text/javascript">
    console.log("!!! before load");
    $(document).ready(function () {
        console.log("load complete");
        require(['ViewModels/StudentEditViewModel'], function (studentEditViewModel) {
            console.log("Initializing");
            var StudentEditViewModelObj = new studentEditViewModel("@ViewBag.Id");
            StudentEditViewModelObj.Initialize();
        });
    });
</script>
