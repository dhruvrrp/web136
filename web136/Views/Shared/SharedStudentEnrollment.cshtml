@{
    var id = ViewBag.Id;
    var studentID = ViewBag.studentID;
}
<h3>Current Enrollment</h3>
StudentID: @studentID
@id

<div id="divSharedStudentEnrollment">
    <table>
        <thead>
            <tr>
                <th></th>
                <th></th>
                <th>Class Title</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: viewModel">
            <tr>
                <td></td>
                <td></td>
                <td data-bind="text: title"></td>
            </tr>
        </tbody>
    </table>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        require(['ViewModels/SharedStudentEnrollmentViewModel'], function (sharedStudentEnrollmentViewModel) {
            var sharedStudentEnrollmentViewModelObj = new sharedStudentEnrollmentViewModel();
            sharedStudentEnrollmentViewModelObj.Load("@studentID");
        });
    });
